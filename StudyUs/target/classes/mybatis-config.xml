<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<!-- null이 전달되면 빈칸이 아닌 null로 인식하도록 설정 -->
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>    
		<typeAlias type="com.studyus.study.domain.Study" alias="Study" />
		<typeAlias type="com.studyus.study.domain.StudySearchCriteria" alias="StudySearchCriteria" />
		<typeAlias type="com.studyus.attendance.domain.Attendance" alias="Attendance" />
		<typeAlias type="com.studyus.meeting.domain.Meeting" alias="Meeting" />
		<typeAlias type="com.studyus.member.domain.Member" alias="Member" />
		<typeAlias type="com.studyus.board.domain.Board" alias="Board"/>
		<typeAlias type="com.studyus.notice.domain.Notice" alias="Notice"/>
		<typeAlias type="com.studyus.purchase.domain.Purchase" alias="Purchase"/>
		<typeAlias type="com.studyus.assignment.domain.Assignment" alias="Assignment"/>
		<typeAlias type="com.studyus.assignment.domain.AssignmentGroup" alias="AssignmentGroup"/>
		<typeAlias type="com.studyus.assignment.domain.Assign" alias="Assign"/>
		<typeAlias type="com.studyus.submittedAssignment.domain.SubmittedAssignment" alias="SubmittedAssignment"/>
		<typeAlias type="com.studyus.file.domain.FileVO" alias="FileVO"/>
		<typeAlias type="com.studyus.enrollment.domain.Enrollment" alias="Enrollment" />
		<typeAlias type="com.studyus.cafe.domain.Cafe" alias="Cafe" />
		<typeAlias type="com.studyus.review.domain.Review" alias="Review" />
		<typeAlias type="com.studyus.caferoom.domain.Caferoom" alias="Caferoom" />
		<typeAlias type="com.studyus.reservation.domain.Reservation" alias="Reservation" />
		<typeAlias type="com.studyus.reservation.domain.ReservationMember" alias="ReservationMember" />
	</typeAliases>
	
	<mappers> 
		<mapper resource="mappers/study-mapper.xml" />
		<mapper resource="mappers/attendance-mapper.xml" />
		<mapper resource="mappers/meeting-mapper.xml" />
		<mapper resource="mappers/member-mapper.xml"/>
		<mapper resource="mappers/hashtag-mapper.xml"/>
		<mapper resource="mappers/board-mapper.xml"/>
		<mapper resource="mappers/notice-mapper.xml"/>
		<mapper resource="mappers/purchase-mapper.xml"/>
		<mapper resource="mappers/assignment-mapper.xml"/>
		<mapper resource="mappers/submittedAssignment-mapper.xml"/>
		<mapper resource="mappers/file-mapper.xml"/>
		<mapper resource="mappers/enrollment-mapper.xml"/>
		<mapper resource="mappers/cafe-mapper.xml"/>
		<mapper resource="mappers/review-mapper.xml"/>
		<mapper resource="mappers/caferoom-mapper.xml"/>
		<mapper resource="mappers/reservation-mapper.xml"/>
	</mappers>  
</configuration> 
